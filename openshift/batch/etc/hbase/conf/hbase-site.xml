<?xml version="1.0" ?>
<configuration>
    <property>
        <name>hbase.rootdir</name>
        <value>hdfs://services.example.com:9000/hbase</value>
    </property>
    <property>
        <name>hbase.cluster.distributed</name>
        <value>true</value>
    </property>
    <property>
        <name>hbase.zookeeper.quorum</name>
        <value>services.example.com:2181</value>
    </property>
    <property>
        <name>hbase.client.metrics.enable</name>
        <value>true</value>
    </property>
</configuration>